{
  "manifest_version": 2,

  "name": "Text",
  "description": "AutoComplete?",
  "version": "0.5",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "app.html"
  },

  "permissions": ["storage", "tabs", "http://*/*", "https://*/*", "file:///*/*"],

  "content_scripts": [
    {
        "matches": [ "http://*/*", "https://*/*", "file:///*/*"],
        "css": ["injectedCss.css"],
        "js": ["jquery-1.10.1.js", "fuse.min.js", "hashmap.js", "eventController.js", "compiled.js"],
        "all_frames": true
    }
  ],
  "background": {
    "scripts": ["storage.js"]
  },
  "web_accessible_resources": ["injectedCss.css", "jquery-1.10.1.js", "fuse.min.js", "hashmap.js", "eventController.js", "compiled.js"]
}
